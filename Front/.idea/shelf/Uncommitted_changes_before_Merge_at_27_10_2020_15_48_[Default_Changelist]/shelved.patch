<+>@import url('https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,200;0,300;0,400;0,500;0,600;1,100;1,200;1,300;1,400;1,500;1,600&display=swap');\r\n\r\n/* VARIABLES CSS*/\r\n:root {\r\n    --color_body: #232427;\r\n    --color_caja: #2a2b2f;\r\n    --color_border_caja: #1e1f23;\r\n    --color_letra_content: rgba(255,255,255,0.05);\r\n    --color_letrah3_content: rgba(255,255,255,0.5);\r\n    --main_font:  'Poppins', sans-serif;\r\n}\r\n\r\n/* ESTILOS UNIVERSALES*/\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    font-family: var(--main_font);\r\n\r\n}\r\n\r\nbody {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-wrap: wrap;\r\n    min-height: 100vh;\r\n    background: var(--color_body);\r\n}\r\n\r\n/* ESTILOS DEL CONTENEDOR */\r\n.container {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-wrap: wrap;\r\n    max-width: 1200px;\r\n    margin: 40px 0;\r\n}\r\n\r\n.container .card {\r\n    position: relative;\r\n    min-width: 320px;\r\n    height: 440px;\r\n    box-shadow: inset 5px 5px 5px rgba(0, 0, 0, 0.2),\r\n                inset -5px -5px 15px rgba(255, 255, 255, 0.1),\r\n                5px 5px 15px rgba(0, 0, 0, 0.3),\r\n                -5px -5px 15px rgba(255, 255, 255, 0.1);\r\n    border-radius: 15px;\r\n    margin: 30px;\r\n}\r\n\r\n.container .card:before {\r\n    position: absolute;\r\n    content: '';\r\n    top: 0;\r\n    left: 0;\r\n    width: 50%;\r\n    height: 100%;\r\n    background: rgba(255, 255, 255, 0.1);\r\n}\r\n\r\n.container .card .box {\r\n    position: absolute;\r\n    top: 20px;\r\n    left: 20px;\r\n    right: 20px;\r\n    bottom: 20px;\r\n    background: var(--color_caja);\r\n    border: 2px solid var(--color_border_caja);\r\n    border-radius: 15px;\r\n    box-shadow: 0 20px 50px rgba(0, 0, 0, 0.5);\r\n    transition: 0.5s;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    overflow: hidden;\r\n}\r\n\r\n.container .card .box:before {\r\n    position: absolute;\r\n    content: '';\r\n    top: 0;\r\n    left: 0;\r\n    width: 50%;\r\n    height: 100%;\r\n    background: rgba(255, 255, 255, 0.05);\r\n    pointer-events: none;\r\n}\r\n\r\n.container .card .box:hover {\r\n    transform: translateY(-50px);\r\n    box-shadow: 0 40px 70px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.container .card .box .content {\r\n    padding: 20px;\r\n    text-align: center;\r\n\r\n}\r\n\r\n.container .card .box .content h2 {\r\n    position: absolute;\r\n    top: -10px;\r\n    right: 30px;\r\n    font-size: 8em;\r\n    color: var(--color_letra_content);\r\n\r\n}\r\n\r\n.container .card .box .content h3 {\r\n    font-size: 1.8em;\r\n    color: var(--color_letrah3_content);\r\n    z-index: 1;\r\n    transition: 0.5s;\r\n}\r\n\r\n.container .card .box .content p {\r\n    font-size: 16px;\r\n    font-weight: 300;\r\n    color: var(--color_letrah3_content);\r\n    z-index: 1;\r\n    transition: 0.5s;\r\n}\r\n\r\n.container .card .box .content a {\r\n    position: relative;\r\n    display: inline-block;\r\n    padding: 8px 20px;\r\n    background: #000;\r\n    margin-top: 15px;\r\n    border-radius: 20px;\r\n    text-decoration: none;\r\n    font-weight: 400;\r\n    color: #fff;\r\n    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.container .card:nth-child(1) .box .content a {\r\n    background: #2196f3;\r\n}\r\n\r\n.container .card:nth-child(2) .box .content a {\r\n    background: #e91e63;\r\n}\r\n\r\n.container .card:nth-child(3) .box .content a {\r\n    background: #97dc47;\r\n}\r\n\r\n.container .card:nth-child(4) .box .content a {\r\n    background-color: #3edddd;\r\n}\r\n\r\n
